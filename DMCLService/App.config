<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="consoleListener" type="System.Diagnostics.ConsoleTraceListener"/>
        <add name="textWriterListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="dmclservice.log"/>
        <add name="eventLogListener" type="System.Diagnostics.EventLogTraceListener" initializeData="DMCLService" />
        <remove name="Default"/>
      </listeners>
    </trace>
  </system.diagnostics>

</configuration>

